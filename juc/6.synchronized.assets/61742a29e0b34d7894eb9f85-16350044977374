function resizeMax(){
Designer.zoomIn();
var current = Designer.config.scale;
$(".embed_resize").children("label").text(parseInt(current * 100) + "%");
}
function resizeMin(){
Designer.zoomOut();
var current = Designer.config.scale;
$(".embed_resize").children("label").text(parseInt(current * 100) + "%");
}
